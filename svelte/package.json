  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otterdev-io/sanity-image-component.git"
  },
  "homepage": "https://github.com/otterdev-io/sanity-image-component/tree/main/svelte#readme",
  "author": "Otterdev",
