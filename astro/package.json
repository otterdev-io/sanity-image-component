{
  "name": "@sanity-image-component/astro",
  "version": "0.1.0",
  "description": "An Astro component for rendering a responsive <img> element for an image fetched from Sanity",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./integration": {
      "types": "./dist/integration.d.ts",
      "module": "./dist/integration.js",
      "default": "./dist/integration.js"
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && cp src/*.astro dist",
    "check": "tsc --noEmit -p ."
  },
  "dependencies": {
    "@sanity-image-component/lib": "^0.1.0",
    "@sanity/image-url": "^1.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.7.0",
    "astro": "^3.0.13",
    "eslint": "^8.49.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "peerDependencies": {
    "@sanity/astro": "> 2.0.0",
    "@sanity/client": "> 6.0.0",
    "@sanity/image-url": "> 1.0.0",
    "@sanity-image-component/lib": "^0.1.0",
    "astro": "> 2.0.0"
  },
  "keywords": [
    "astro",
    "astro-component",
    "astro-components",
    "sanity",
    "image",
    "responsive"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otterdev-io/sanity-image-component.git"
  },
  "homepage": "https://github.com/otterdev-io/sanity-image-component#readme",
  "author": "Otterdev",
  "maintainers": [
    {
      "name": "Christopher Fraser",
      "email": "chris@otterdev.io"
    }
  ],
  "license": "ISC"
}
